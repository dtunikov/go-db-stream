{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "parse benckmark.log",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/benchmark/postgres-kafka/main.go",
      "env": {
        "BENCHMARK": "true"
      }
    },
    {
      "name": "Run server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "-config", "local.yaml",
      ]
    }
  ]
}